<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.entgroup.adms.mapper.ActiveCodeMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.entgroup.adms.model.system.ActiveCode">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="active_code" property="activeCode" />
		<result column="type" property="type" />
		<result column="used" property="used" />
		<result column="expires_time" property="expiresTime" />
		<result column="create_time" property="createTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, active_code AS activeCode, type, used, expires_time AS expiresTime, create_time AS createTime
    </sql>
</mapper>
